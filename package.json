{
  "name": "lamp-lighting",
  "version": "0.0.1",
  "description": "Lamp Lighting project",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "autoprefixer": "^10.3.1",
